   "source": [
    "def _push(evm, n):\n",
    "    evm.pc += 1\n",
    "    evm.gas_dec(3)\n",
    "    \n",
    "    value = []\n",
    "    for _ in range(n):\n",
    "        value.append(evm.peek())\n",
    "        evm.pc += 1\n",
    "    evm.stack.push(int(''.join(map(str, value))))"
   ]
  }
