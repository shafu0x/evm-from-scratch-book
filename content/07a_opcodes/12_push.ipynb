    "# Push"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "a6df5a45",
   "metadata": {},
   "source": [
    "Push items to the EVM stack."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "b80b0357",
   "metadata": {},
   "outputs": [],
   "source": [
    "def _push(evm, n):\n",
    "    evm.pc += 1                 \n",
    "    evm.gas_dec(3)\n",
    "\n",
    "    value = 0\n",
    "    for _ in range(n):\n",
    "        value = (value << 8) | evm.peek()\n",
    "        evm.pc += 1\n",
    "\n",
    "    evm.stack.push(value % MOD) # enforce 256-bit wraparound"
   ]
  }
 ],
