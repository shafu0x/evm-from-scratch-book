   "source": [
    "pub fn revert(evm: &mut EVM) {\n",
    "    let offset = evm.stack.pop();\n",
    "    let size = evm.stack.pop();\n",
    "    evm.returndata = evm.memory.access(offset, size);\n",
    "\n",
    "    evm.stop_flag = true;\n",
    "    evm.revert_flag = true;\n",
    "    evm.pc += 1;\n",
    "    evm.gas_dec(0);\n",
    "}"
   ]
  }
